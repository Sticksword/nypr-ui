{{#each groups as |group index|}}
  {{#if (eq index 0)}}
    {{nypr-brick-image-row items=group audio=audio chunk=chunk}}
  {{else if (eq index 1)}}
    {{nypr-brick-card-row items=group audio=audio}}
  {{else}}
    {{nypr-brick-image-row items=group flip=(eq index 3) audio=audio}}
  {{/if}}
{{/each}}
